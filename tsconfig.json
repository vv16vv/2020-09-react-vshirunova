{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "styled/*": ["src/components/styled/*"],
      "cmp/*": ["src/components/*"],
      "@/*": ["src/*"]
    },
    "target": "es5",
    "module": "commonjs",
    "allowJs": true,
    "jsx": "react",
    "sourceMap": true,
    "strict": true,
    "esModuleInterop": true,
    "outDir": "dist"
  }
}
